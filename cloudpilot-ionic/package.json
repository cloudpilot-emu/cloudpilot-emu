{
  "name": "cloudpilot-ionic",
  "version": "1.0.0-beta1",
  "author": "Christian Speckner",
  "homepage": "https://cloudpilot-emu.github.io/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn run gen-revision && ng run app:build",
    "gen-revision": "echo \"export const REVISION = '`git rev-parse --short HEAD`';\" > src/revision.gen.ts",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "pwa-icons": "pwa-asset-generator --opaque false --favicon --scrape --icon-only -m src/manifest.webmanifest -i src/index.html assets-pwa-src/icon.html src/assets-pwa",
    "pwa-splash": "pwa-asset-generator --scrape --splash-only -m src/manifest.webmanifest -i src/index.html assets-pwa-src/splashscreen.html src/assets-pwa",
    "pwa-splash-dark": "pwa-asset-generator --scrape --splash-only --dark-mode -m src/manifest.webmanifest -i src/index.html assets-pwa-src/splashscreen.html src/assets-pwa",
    "pwa-assets": "yarn pwa-icons && yarn pwa-splash && yarn pwa-splash-dark && prettier -w --parser html src/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@angular/service-worker": "~11.2.5",
    "@ionic/angular": "^5.6.0",
    "@types/deep-equal": "^1.0.1",
    "@types/md5": "^2.3.0",
    "@types/uuid": "^8.3.0",
    "async-mutex": "^0.3.1",
    "deep-equal": "^2.0.5",
    "md5": "^2.3.0",
    "microevent.ts": "^0.2.1",
    "ngx-markdown": "^11.1.2",
    "rxjs": "~6.6.6",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.4",
    "@angular/cli": "~11.2.4",
    "@angular/compiler": "~11.2.5",
    "@angular/compiler-cli": "~11.2.5",
    "@angular/language-service": "~11.2.5",
    "@ionic/angular-toolkit": "^3.1.0",
    "@types/emscripten": "^1.39.4",
    "@types/jasmine": "~3.6.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^14.14.35",
    "codelyzer": "^6.0.0",
    "import-sort-style-eslint": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.2.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "prettier-plugin-import-sort": "^0.0.6",
    "protractor": "~7.0.0",
    "pwa-asset-generator": "^4.1.1",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.0"
  },
  "description": "Cloudpilot PWA",
  "prettier": {
    "printWidth": 120,
    "tabWidth": 4,
    "singleQuote": true,
    "parser": "typescript"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "eslint",
      "parser": "typescript"
    }
  }
}
